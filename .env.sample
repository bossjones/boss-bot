# Discord Configuration
DISCORD_TOKEN=Bot your_token_here
DISCORD_CLIENT_ID=123456789
DISCORD_SERVER_ID=123456789
DISCORD_ADMIN_USER_ID=123456789
DISCORD_ADMIN_USER_INVITED=false

# Feature Flags
ENABLE_AI=false
ENABLE_REDIS=false
ENABLE_SENTRY=false

# Service Settings
SENTRY_DSN=
OPENAI_API_KEY=sk-your_key_here

# Storage Configuration
STORAGE_ROOT=/tmp/boss-bot
MAX_FILE_SIZE_MB=50
MAX_CONCURRENT_DOWNLOADS=5
MAX_QUEUE_SIZE=50

# Monitoring Configuration
LOG_LEVEL=INFO
ENABLE_METRICS=true
METRICS_PORT=9090
ENABLE_HEALTH_CHECK=true
HEALTH_CHECK_PORT=8080

# Security Configuration
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW_SECONDS=60
ENABLE_FILE_VALIDATION=true

# Development Settings
DEBUG=false
ENVIRONMENT=development

# Additional API Keys
COHERE_API_KEY=your_key_here
DEBUG_AIDER=false
FIRECRAWL_API_KEY=your_key_here
LANGCHAIN_API_KEY=your_key_here
LANGCHAIN_DEBUG_LOGS=false
LANGCHAIN_ENDPOINT=http://localhost:8000
LANGCHAIN_HUB_API_KEY=your_key_here
LANGCHAIN_HUB_API_URL=http://localhost:8001
LANGCHAIN_PROJECT=test-project
LANGCHAIN_TRACING_V2=false
PINECONE_API_KEY=your_key_here
PINECONE_ENV=test-env
PINECONE_INDEX=test-index
TAVILY_API_KEY=your_key_here
UNSTRUCTURED_API_KEY=your_key_here
UNSTRUCTURED_API_URL=http://localhost:8002
